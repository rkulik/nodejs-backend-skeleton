{
  "moduleNameMapper": {
    "^@configs/(.*)$": "<rootDir>/src/configs/$1",
    "^@modules/(.*)$": "<rootDir>/src/modules/$1",
    "^@setup/(.*)$": "<rootDir>/src/setup/$1",
    "^@src/(.*)$": "<rootDir>/src/$1"
  },
  "preset": "ts-jest",
  "setupFilesAfterEnv": ["<rootDir>/jest.setup.ts"],
  "testEnvironment": "node",
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  }
}
